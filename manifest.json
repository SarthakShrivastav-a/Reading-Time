{
  "manifest_version": 3,
  "name": "Reading Time",
  "version": "1.0",
  "description": "Predict the reading time of articles on any website.",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": ["<all_urls>"], 
      "run_at": "document_idle"  
    }
  ]
}
